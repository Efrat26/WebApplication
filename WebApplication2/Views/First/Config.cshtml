@model IEnumerable<WebApplication2.Models.ConfigModel>
@{
    ViewBag.Title = "Config";
}
<script>
    function myNewFunction(sel) {
        //alert(sel.options[sel.selectedIndex].text);
        window.location.replace("DeleteHandler", "DeleteHandler");
    }
</script>
<h2>App Config Properties</h2>
<p>Output Directory Path:  @Model.ElementAt(0).OutputDirectory</p>
<p>Log Name:  @Model.ElementAt(0).LogName</p>
<p>Source Name:  @Model.ElementAt(0).SourceName</p>
<p>Thumbnail Size:  @Model.ElementAt(0).ThumbnailSize</p>
<p> handlers:</p>
<select id="handlersList" onchange="myNewFunction(this)">
    @for (int i = 0; i < Model.ElementAt(0).Handlers.Count; ++i)
    {
    <option >@Model.ElementAt(0).Handlers.ElementAt(i).ToString()</option>
    }
    
</select>

